    "chai-spies": "^1.0.0",
    "dockerlint": "^0.3.9",
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "ioredis-mock": "^4.21.2",
    "markdownlint-cli": "^0.23.2",
    "mocha": "^8.1.0",
    "mongo-mock": "^4.0.0",
    "mongodb-memory-server": "^6.6.3",
    "nock": "^13.0.3",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.4",
    "npm-check-updates": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "sinon": "^9.0.2",
    "why-is-node-running": "^2.2.0",
    "yaml-lint": "^1.2.4"
  }
}
