{
  "name": "razeedash-api",
  "version": "0.0.1",
  "description": "API for razeedash",
  "license": "Apache-2.0",
  "main": "app/index.js",
  "scripts": {
    "start": "node app/index.js",
    "dev": "nodemon -r dotenv/config app/index.js | bunyan",
    "start:mon": "nodemon app/index.js | bunyan",
    "start:debug": "node --inspect-brk app/index.js",
    "test": "nyc --all --exclude=**/*.tests.js --exclude=coverage/* --reporter=html --reporter=text mocha -r dotenv/config **/*.tests.js",
    "wait-mongo": "node app/wait-mongo.js",
    "lint": "npx npm-run-all eslint yamllint dockerlint markdownlint",
    "eslint": "npx eslint app/",
    "yamllint": "npx yamllint .travis.yml kubernetes/razeedash-api/resource.yaml",
    "dockerlint": "npx dockerlint Dockerfile",
    "markdownlint": "npx markdownlint README.md docs/"
  },
  "keywords": [],
  "author": "",
  "contributors": [],
  "dependencies": {
    "aws-sdk": "^2.527.0",
    "bunyan": "^1.8.12",
    "clone": "^2.1.2",
    "compression": "^1.7.4",
    "crypto-js": "^3.1.9-1",
    "delay": "^4.3.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-bunyan-logger": "^1.3.3",
    "express-query-boolean": "^2.0.0",
    "express-request-id": "^1.4.1",
    "fs-readfile-promise": "^3.0.1",
    "json-key-validate": "^1.0.2",
    "lodash": "^4.17.14",
    "mongodb": "^3.3.4",
    "mustache": "^3.0.1",
    "object-hash": "^2.0.0",
    "object-path": "^0.11.4",
    "p-limit": "^2.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "dockerlint": "^0.3.9",
    "dotenv": "^8.2.0",
    "eslint": "^6.0.1",
    "markdownlint-cli": "^0.19.0",
    "mocha": "^6.1.4",
    "mongo-mock": "^3.7.1",
    "nock": "^11.1.0",
    "node-mocks-http": "^1.7.5",
    "nodemon": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rewire": "^4.0.1",
    "yaml-lint": "^1.2.4"
  }
